;
; Netatalk 3.x configuration file
;

[Global]
; log level = default:maxdebug
log file = /var/log/netatalk
uam list = uams_dhx.so uams_dhx2.so

[Homes]
basedir regex = /home

<% shares.each do |share| %>
[<%= share.name %>]
path = <%= share.path %>
time machine = <%= share.options.time_machine ? 'yes' : 'no' %>
<% end %>
